try { 
	/*

Ticket link: https://app.asana.com/0/home/1205991754603481/1206762901748865 

*/

webengage.notification.onClick(function (data) {
    if (data.notificationId === '31760329') {
     console.log("notification called");
         setTimeout(function(){
        webengage.notification.render({ notificationId : '31760329',forcedRender : true });
         },10000);
    }
});

 } catch(e) { 
 	if (e instanceof Error) { 
		var data = e.stack || e.description;
		data = (data.length > 900 ? data.substring(0, 900) : data);
	 	webengage.eLog(null, 'error', data, 'cwc-error','cwc', '~5bjla1f');
	 }
 }
